alias du='du -kh'

# ls extensions
alias lt='ls -ltr'	# Sort by date, most recent last
alias lk='ls -lSr'	# Sort by size, largest last

# Git commands
alias gs='git status'
alias gd='git diff'
alias gb='git branch'
alias gl='git log --oneline --decorate --graph'

# Fix typos
alias kk='ll'

# Docker
alias d=docker
alias dc=docker-compose
alias dps='docker ps | docker-format'
